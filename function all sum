#include<stdio.h>
#include<conio.h>
void sum(int num1,int num2);
int main()
{
	int num1;
	int num2;
	printf("Enter num1 :");
    scanf("%d%d",&num1,&num2);
	sum(num1,num2);
	
}
void sum(int n1,int n2)
{
	int add=n1+n2;
	int sub=n1-n2;
	int mul=n1*n2;
	int div=n1/n2;
	{
    	printf("sum = %d\n",add);
    	printf("sum = %d\n",sub);
    	printf("sum = %d\n",mul);
    	printf("sum = %d\n",div);
    	
	}
    	
}
